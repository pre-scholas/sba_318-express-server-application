<%- include('partials/header') %>
    <h1>All Users</h1>
    <ul>
        <% users.forEach(user => { %>
            <li>
                <strong><%= user.name %></strong> (<%= user.username %>) - <%= user.email %>
                <br>
                <a href="/view/users/<%= user.id %>/posts">View Posts</a>
            </li>
        <% }) %>
    </ul>
    <br>
    <a href="/">Back to all posts</a>
<%- include('partials/footer') %>